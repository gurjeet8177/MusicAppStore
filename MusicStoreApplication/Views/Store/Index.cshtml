@model IEnumerable<MusicStoreApplication.Models.Genre>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<ul>
       @foreach (var g in Model)
       {
           <li>@Html.ActionLink(g.Name,"Details","Store", new { genre=g.Name }, null)</li>
       }
</ul>